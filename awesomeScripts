

######################################################
CHANGE THE FILES

######################################################
for i in ` find /root/test/ -name "*.shp" ` ;
do
  mv $i ` echo $i | sed 's/.shp$/_poly.shp/g' ` ;
done




################################

TCP DUMP

sudo tcpdump -i eth0 -n -vv>temp
sudo tcpdump -D
sudo tcpdump -i -c 1000 -v
sudo tcpdump -i eth0 -s0
